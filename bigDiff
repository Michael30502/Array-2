public int bigDiff(int[] nums) {
  
  int  result = 0;
 int  biggest=0;
 int  smallest= 10000;
  
  
  if(nums.length > 0){
    for(int i = 0;i<nums.length;i++){
     
    biggest =  Math.max(biggest,nums[i]);
    smallest = Math.min(smallest,nums[i]);
     
     result = (biggest-smallest);
      
    }
  }
  
  return result;
}
